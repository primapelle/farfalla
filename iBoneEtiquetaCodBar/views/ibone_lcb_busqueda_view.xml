<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="model_ibone_lcb_busqueda_form" model="ir.ui.view">
        <field name="name">modelo.ibone.lcb.busqueda.form</field>
        <field name="model">cok.ibone.lotecodebar.busqueda</field>
        <field name="arch" type="xml">
            <form string="Proceso de búsqueda">
                <sheet>
                    <div><p>Ingrese el folio a buscar y tabule para comenzar la operación</p></div>
                    <div>
                        <field name="codigoBusqueda" placeholder="Lote por localizar" />
                    </div>
                    <div style="margin: 10px;">
                        <b><field name="resultadoBusqueda" placeholder="Resultado" /></b>  
                    </div>
                    <div style="margin-left: 10px;">
                        <b><field name="resultadoBusqueda_id" readonly="1" /></b>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
